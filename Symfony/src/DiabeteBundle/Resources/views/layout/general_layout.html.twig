<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Blank page | Nifty - Responsive admin template</title>

{% block include %} {% endblock %}


</head>
<body>
<div id="container" class="effect mainnav-lg">

	{% block navbar %}{% endblock %}


    <!-- CONTENT -->
    <!--===================================================-->
    <div class="boxed">


        <!-- CONTENT CONTAINER -->
        <!--===================================================-->
        <div id="content-container">

            <!-- PAGE TITTLE -->
            <!--===================================================-->
            <div id="page-title">
                <h1 class="page-header text-overflow"> {% block page_title %}{% endblock %}</h1>





            </div>
            <!--===================================================-->
            <!-- END OF PAGE TITLE -->

    {% block breadcrumb %}{% endblock %}





            <!-- PAGE CONTENT -->
            <!--===================================================-->
            <div id="page-content">

		{% block content %}{% endblock %}


            </div>
            <!--===================================================-->
            <!-- END OF PAGE CONTENT -->


        </div>
        <!--===================================================-->
        <!-- END OF CONTENT CONTAINER -->

{% block side_bar_menu %} {% endblock %}



{% block additionnal_side_bar %} {% endblock %}


    </div>
    <!--===================================================-->
    <!--END OF CONTENT -->






	<!-- FOOTER -->
	<!--===================================================-->

	<!--===================================================-->
	<!-- END OF FOOTER -->



	<!-- SCROLL TOP BUTTON -->
	<!--===================================================-->
	<button id="scroll-top" class="btn"><i class="fa fa-chevron-up"></i></button>
	<!--===================================================-->




</div>
<!--===================================================-->
<!-- END OF CONTAINER -->





</body>

{% block script %} {% endblock %}
</html>